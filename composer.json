{
    "name": "bscheshir/yii-arctic-modal",
    "description": "Yii extension wrapper for arctic-modal",
    "license": "MIT",
    "authors": [
        {
            "name": "BSCheshir",
            "email": "bscheshir@gmail.com"
        }
    ],
    "repositories": [
